{
  "schemaVersion": 1,
  "id": "${id}",
  "version": "${version}",
  "name": "${name}",
  "description": "Better fish experience",
  "authors": [
    "c0nstexpr"
  ],
  "contact": {
    "homepage": "https://github.com/c0nstexpr",
    "sources": "https://github.com/c0nstexpr/fishology"
  },
  "license": "MIT",
  "environment": "client",
  "entrypoints": {
    "client": [
      {
        "adapter": "kotlin",
        "value": "org.c0nstexpr.fishology.core.FishologyCoreModKt::init"
      }
    ]
  },
  "mixins": [
    "fishology.core.mixins.json"
  ],
  "icon": "assets/${id}/icon.png",
  "depends": {
    "minecraft": "${minecraft}",
    "fabric": ">=${fabricApi}",
    "fabric-screen-api-v1": ">=${fabricScreenApi}",
    "fabric-key-binding-api-v1": ">=${fabricKeyBindingApi}",
    "fabric-language-kotlin": ">=${fabricKotlin}",
    "owo": ">=${owo}",
    "modmenu": ">=${modmenu}"
  }
}